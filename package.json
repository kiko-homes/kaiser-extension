{
  "name": "kaiser-extension",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@craco/craco": "^6.4.3",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "eventemitter3": "^4.0.7",
    "firebase": "^9.6.9",
    "framer-motion": "^6",
    "i18next": "^21.6.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.16.1",
    "react-icons": "^4.3.1",
    "react-scripts": "5.0.0",
    "react-shadow": "^19.0.2",
    "single-file": "git+https://github.com/gildas-lormeau/SingleFile-Lite.git",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4",
    "zustand": "^3.7.1"
  },
  "scripts": {
    "start": "BROWSER=none PORT=4000 craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "lint:fix": "yarn lint --fix",
    "cm": "cz",
    "postinstall": "husky install",
    "build:prod": "rm kaiser.zip ; NODE_ENV=development yarn build && zip -r kaiser.zip build",
    "build:prod:test": "rm kaiser-prod.zip ; NODE_ENV=production-test yarn build && zip -r kaiser-prod.zip build",
    "build:dev": "rm kaiser-dev.zip ; NODE_ENV=development yarn build && zip -r kaiser-dev.zip build",
    "build:test": "yarn build:prod:test && yarn build:dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/chrome": "^0.0.179",
    "@types/offscreencanvas": "^2019.6.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^10.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "watch": "^1.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">= 16.10.0"
  }
}
